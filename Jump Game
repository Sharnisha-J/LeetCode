class Solution {
    public boolean canJump(int[] nums) {
        int n=nums.length;
        int maxreach=0;
        for(int row=0;row<=n-2;row++){
            if(maxreach>=row){
                maxreach=Math.max(maxreach,row+nums[row]);
            }
        }
        return maxreach>=n-1;
    }
}
